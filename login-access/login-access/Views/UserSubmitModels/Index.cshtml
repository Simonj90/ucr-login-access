@model IEnumerable<login_access.Models.UserSubmitModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FreeText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoolDB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AwesomeDB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FinishedSubmit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReturnText)
        </th>
