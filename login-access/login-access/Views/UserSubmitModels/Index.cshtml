@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FreeText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoolDB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AwesomeDB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FinishedSubmit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReturnText)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
